<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module>

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="ru.intertrust.cm.core.gui.api.Api"/>
    <inherits name="ru.intertrust.cm.core.gui.GuiModel"/>
    <inherits name="ru.intertrust.cm.core.gui.impl.Login"/>

    <entry-point class='ru.intertrust.cm.core.gui.impl.client.BusinessUniverse'/>

    <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
    <inherits name="com.google.gwt.user.theme.chrome.Chrome"/>
    <inherits name="ru.intertrust.cm.core.config.Configuration"/>

    <source path="client"/>
    <set-property name="gwt.logging.logLevel" value="ALL"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>
    <set-property name="gwt.logging.popupHandler" value="DISABLED" />

</module>