    <xs:complexType name="applicationHelpType">
        <xs:attribute name="source" use="optional" type="xs:string"/>
    </xs:complexType>

    <xs:complexType name="themesType">
        <xs:sequence minOccurs="1">
            <xs:element name="theme" type="con:themeType" maxOccurs="unbounded" minOccurs="1"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="languagesType">
        <xs:sequence minOccurs="1">
            <xs:element name="language" type="con:languageType" maxOccurs="unbounded" minOccurs="1"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
