            <xs:element type="con:criteriaType" name="criteria"
                        minOccurs="0" xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
        </xs:sequence>
        <xs:attribute type="con:filterNameType" name="name" use="optional"/>
