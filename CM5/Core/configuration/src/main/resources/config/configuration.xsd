<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified"
           elementFormDefault="qualified" targetNamespace="https://cm5.intertrust.ru/config"
           xmlns:con="https://cm5.intertrust.ru/config"
           xmlns:act="https://cm5.intertrust.ru/config/action"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:import namespace="https://cm5.intertrust.ru/config/action" schemaLocation="actions.xsd"/>

    <xs:element name="configuration" type="con:configurationType"/>

    <xs:element name="widget" type="con:abstractWidgetType"/>
    <xs:complexType name="abstractWidgetType" abstract="true">
        <xs:attribute name="read-only" use="optional" type="xs:boolean"/>
        <xs:attribute type="xs:string" name="max-tooltip-width" use="optional"/>
        <xs:attribute type="xs:string" name="max-tooltip-height" use="optional"/>
    </xs:complexType>

    <xs:element name="pluginConfig" type="con:abstractPluginConfigType"/>
    <xs:complexType name="abstractPluginConfigType" abstract="true"/>

    <xs:complexType name="longType">
        <xs:sequence>
            <xs:element name="constraints" type="con:constraintsType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
        <xs:attribute name="name" type="con:fieldNameType"
                      xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute name="not-null" use="optional" type="xs:string"/>
        <xs:attribute name="immutable" use="optional" type="xs:boolean"/>

    </xs:complexType>

    <xs:complexType name="domainObjectTypeToValidate">
        <xs:sequence>
            <xs:element name="fields" minOccurs="0" maxOccurs="1" type="con:fieldsType"
                        xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
            <xs:element type="con:uniqueKeyType" name="uniqueKey"
                        minOccurs="0" maxOccurs="unbounded"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element type="con:indecesType" name="indices"
                        minOccurs="0" maxOccurs="unbounded"/>
            <xs:element type="con:attachmentTypes" name="attachment-types"
                        minOccurs="0" maxOccurs="1"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
        <xs:attribute name="name" use="required" type="con:domainObjectNameType"
                      xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute name="db-id" use="optional" type="xs:integer"
                      xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute type="xs:string" name="extends" use="optional"/>
        <xs:attribute type="xs:boolean" name="template"
                      use="optional"/>
        <xs:attribute type="xs:boolean" name="audit-log"
                      use="optional"/>
        <xs:attribute name="initial-status" use="optional"
                      type="xs:string" xmlns:con="https://cm5.intertrust.ru/config"/>
    </xs:complexType>

    <xs:complexType name="attachmentTypes">
        <xs:sequence>
            <xs:element type="con:attachmentType" name="attachment-type"
                        minOccurs="1" maxOccurs="unbounded"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="attachmentType">
        <xs:attribute name="name" use="required" type="con:domainObjectNameType"
                      xmlns:con="https://cm5.intertrust.ru/config"/>
    </xs:complexType>

    <xs:complexType name="namedTriggerType">
        <xs:sequence>
            <xs:element name="trigger" minOccurs="0" maxOccurs="1" type="con:triggerType">
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" use="optional" type="con:domainObjectNameType"/>
    </xs:complexType>

    <xs:complexType name="triggerType">
        <xs:choice>
            <xs:element name="config" minOccurs="0" maxOccurs="1" type="con:triggerConfigType"/>
            <xs:element name="class-name" minOccurs="0" maxOccurs="1" type="con:javaClassNameType">
            </xs:element>
            <xs:element name="conditions-script" minOccurs="0" maxOccurs="1" type="xs:string">
            </xs:element>
        </xs:choice>
        <xs:attribute name="domain-object-type" use="optional" type="con:domainObjectNameType"/>
        <xs:attribute name="event" use="optional" type="con:triggerEventType"/>
        <xs:attribute name="refName" type="xs:string" use="optional"/>
    </xs:complexType>

    <xs:complexType name="triggerConfigType">
        <xs:sequence>
            <xs:element name="fields" minOccurs="0" maxOccurs="1" type="con:triggerFieldsType">
            </xs:element>
            <xs:element name="statuses" minOccurs="0" maxOccurs="1" type="con:triggerStatusesType">
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="triggerEventType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CREATE"/>
            <xs:enumeration value="CHANGE"/>
            <xs:enumeration value="CHANGE_STATUS"/>
            <xs:enumeration value="DELETE"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="notificationPriority">
        <xs:restriction base="xs:string">
            <xs:enumeration value="HIGH"/>
            <xs:enumeration value="NORMAL"/>
            <xs:enumeration value="LOW"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="triggerFieldsType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element name="field" type="con:fieldNameType"/>
        </xs:choice>
    </xs:complexType>

    <xs:simpleType name="javaClassNameType">
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:pattern value="[a-zA-Z0-9_.]*">
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="addresseJavaClassNameType">
        <xs:sequence>
            <xs:element name="find-person-settings" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:any minOccurs="1" maxOccurs="1" processContents="skip" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string" use="required" />
    </xs:complexType>

    <xs:complexType name="triggerStatusesType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element name="status" type="con:triggerStatusType"/>
        </xs:choice>
    </xs:complexType>

    <xs:simpleType name="triggerStatusType">
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:pattern
                    value="[a-zA-Z0-9_]*">
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="notificationType">
        <xs:sequence>
            <xs:element name="notification-type" minOccurs="0" maxOccurs="1" type="con:notificationTypeType">
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" use="required" type="con:notificationNameType"/>
    </xs:complexType>

    <xs:complexType name="notificationTypeType">
        <xs:sequence>
            <xs:element name="addressee" minOccurs="0" maxOccurs="1" type="con:notificationAddresseType"/>
            <xs:element name="triggers" minOccurs="0" maxOccurs="1" type="con:notificationTriggersType"/>
        </xs:sequence>
        <xs:attribute name="name" use="required" type="xs:string"/>
        <xs:attribute name="priority" use="required" type="con:notificationPriority"/>
    </xs:complexType>

    <xs:complexType name="notificationAddresseType">
        <xs:choice>
            <xs:element name="find-person" minOccurs="0" maxOccurs="1" type="con:notificationAddresseFindPersonType">
            </xs:element>
            <xs:element name="by-context-role" minOccurs="0" maxOccurs="1" type="xs:string"/>
            <xs:element name="by-dynamic-group" minOccurs="0" maxOccurs="1" type="xs:string"/>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="notificationAddresseFindPersonType">
        <xs:choice>
            <xs:element name="find-query" minOccurs="0" maxOccurs="1" type="xs:string"/>
            <xs:element name="class-name" minOccurs="0" maxOccurs="1" type="con:addresseJavaClassNameType"/>
            <xs:element name="doel" minOccurs="0" maxOccurs="1" type="xs:string"/>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="notificationTriggersType">
        <xs:sequence>
            <xs:element name="trigger" minOccurs="1" maxOccurs="unbounded" type="con:triggerType">
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="notificationNameType">
        <xs:restriction base="xs:string">
            <xs:pattern
                    value="[a-zA-Z0-9_]*">
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="domainObjectNameType">
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
            <xs:pattern value="[a-zA-Z0-9_]*">
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="fieldNameType">
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
            <xs:pattern value="[a-zA-Z0-9_]*">
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="filterNameType">
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
            <xs:pattern value="[a-zA-Z0-9_]*">
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="stringFieldLengthType">
        <xs:restriction base="xs:int">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="4000"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="typeNameType">
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
            <xs:pattern value="[a-zA-Z0-9_]*|\*">
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="indexTypeType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="btree"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="fieldsType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="con:booleanType" name="boolean" xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element type="con:stringType" name="string"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element type="con:textType"
                        name="text" xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
            <xs:element type="con:dateTimeType" name="date-time"
                        xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
            <xs:element type="con:dateTimeWithTimeZoneType"
                        name="dateTimeWithTimeZone" xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
            <xs:element type="con:timelessDateType"
                        name="timelessDate" xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
            <xs:element type="con:referenceType" name="reference"
                        xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
            <xs:element type="con:longType" name="long"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element type="con:decimalType" name="decimal"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element type="con:passwordType" name="password"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="fieldType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="name" use="required"
                              type="con:fieldNameType" xmlns:con="https://cm5.intertrust.ru/config"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="referenceType">
        <xs:attribute name="name" use="required" type="con:fieldNameType"
                      xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute name="type" use="required" type="con:typeNameType"
                      xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute name="not-null" use="optional" type="xs:string"/>
        <xs:attribute name="immutable" use="optional" type="xs:boolean"/>
    </xs:complexType>

    <xs:complexType name="dateTimeType">
        <xs:sequence>
            <xs:element name="constraints" type="con:constraintsType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
        <xs:attribute name="name" use="required"
                      type="con:fieldNameType" xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute name="not-null" use="optional" type="xs:string"/>
        <xs:attribute name="immutable" use="optional" type="xs:boolean"/>

    </xs:complexType>

    <xs:complexType name="dateTimeWithTimeZoneType">
        <xs:sequence>
            <xs:element name="constraints" type="con:constraintsType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
        <xs:attribute name="name" use="required"
                      type="con:fieldNameType" xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute name="not-null" use="optional" type="xs:string"/>
        <xs:attribute name="immutable" use="optional" type="xs:boolean"/>

    </xs:complexType>

    <xs:complexType name="timelessDateType">
        <xs:sequence>
            <xs:element name="constraints" type="con:constraintsType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
        <xs:attribute name="name" use="required"
                      type="con:fieldNameType" xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute name="not-null" use="optional" type="xs:string"/>
        <xs:attribute name="immutable" use="optional" type="xs:boolean"/>
    </xs:complexType>

    <xs:complexType name="configurationType">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element type="con:globalConfigType"
                        name="global-settings" maxOccurs="1" minOccurs="1"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element type="con:domainObjectTypeToValidate" name="domain-object-type"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element type="con:collectionType" name="collection"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="static-group" type="con:staticGroupType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="dynamic-group" type="con:dynamicGroupType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="context-role" type="con:contextRoleType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="access-matrix" type="con:accessMatrixType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="navigation" type="con:navigationPanelType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="form" type="con:formType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="form-mappings" type="con:formMappingsType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="navigation-panel-mappings" type="con:navigationPanelMappingsType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="navigation-panel-mapping" type="con:navigationPanelMappingType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <!--
                        <xs:element name="groups" type="con:groupsNavType"
                                    maxOccurs="unbounded" minOccurs="0"
                                    xmlns:con="https://cm5.intertrust.ru/config"/>
            -->
            <!-- <xs:element name="group-name" type="con:groupNavType"
                         maxOccurs="unbounded" minOccurs="0"
                         xmlns:con="https://cm5.intertrust.ru/config"/>-->
            <xs:element name="widget-template" type="con:widgetTemplateType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="collection-view" type="con:collectionViewType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="collection-mappings" type="con:collectionMappingsType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element ref="act:action"/>
            <xs:element ref="act:tool-bar"/>
            <xs:element ref="act:menu-bar"/>
            <xs:element ref="act:action-context"/>
            <xs:element name="search-area" type="con:searchAreaType"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="business-universe" type="con:businessUniverseType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element type="con:namedTriggerType" name="named-trigger"
                        maxOccurs="unbounded" minOccurs="0"/>
            <xs:element name="url-config" type="con:urlConfigType"
                        minOccurs="0" maxOccurs="unbounded"/>
            <xs:element type="con:notificationType" name="notification"
                        maxOccurs="unbounded" minOccurs="0"/>

        </xs:choice>
    </xs:complexType>

    <xs:complexType name="businessUniverseType">
        <xs:choice maxOccurs="unbounded">
            <xs:element name="logo" type="con:logoType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="settings-popup" type="con:settingsPopupType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="header-notification-refresh"
                        type="con:collectionCountRefreshType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="collection-count-refresh"
                        type="con:collectionCountRefreshType"
                        maxOccurs="1" minOccurs="1"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="collection-count-cache-refresh"
                        type="con:collectionCountCacheRefreshType"
                        maxOccurs="1" minOccurs="1"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="settingsPopupType">
        <xs:sequence minOccurs="1">
            <xs:element name="themes"
                        type="con:themesType"
                        maxOccurs="1" minOccurs="1"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="themesType">
        <xs:sequence minOccurs="1">
            <xs:element name="theme"
                        type="con:themeType"
                        maxOccurs="unbounded" minOccurs="1"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="themeType">
        <xs:attribute name="display-name" use="required" type="xs:string"/>
        <xs:attribute name="component-name" use="required" type="xs:string"/>
        <xs:attribute name="img" use="required" type="xs:string"/>
        <xs:attribute name="default-theme" use="optional" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="collectionCountRefreshType">
        <xs:attribute name="time" use="required" type="xs:integer"/>
    </xs:complexType>
    <xs:complexType name="collectionCountCacheRefreshType">
        <xs:attribute name="time" use="required" type="xs:integer"/>
    </xs:complexType>
    <xs:complexType name="logoType">
        <xs:attribute name="image" use="required" type="xs:string"/>
    </xs:complexType>

    <xs:complexType name="urlConfigType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="name" use="required" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <!-- Global Config-->
    <xs:complexType name="globalConfigType">
        <xs:sequence>
            <xs:element name="audit-log">
                <xs:complexType>
                    <xs:attribute name="enable" use="optional"
                                  type="xs:boolean"/>
                </xs:complexType>
            </xs:element>

            <xs:element name="sql-trace">
                <xs:complexType>
                    <xs:attribute name="enable" use="required"
                                  type="xs:boolean"/>
                    <xs:attribute name="resolveParameters" use="required"
                                  type="xs:boolean"/>
                    <xs:attribute name="minTime" use="optional"
                                  type="xs:integer"/>
                    <xs:attribute name="minRows" use="optional"
                                  type="xs:integer"/>
                </xs:complexType>
            </xs:element>

            <xs:element name="transaction-trace" minOccurs="0">
                <xs:complexType>
                    <xs:attribute name="enable" use="required"
                                  type="xs:boolean"/>
                    <xs:attribute name="minTime" use="optional"
                                  type="xs:integer"/>
                </xs:complexType>
            </xs:element>

            <xs:element name="search-languages" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="language" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="id" use="required" type="xs:string"/>
                                <xs:attribute name="weight" use="optional" type="xs:positiveInteger"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="uniqueKeyType">
        <xs:sequence maxOccurs="unbounded">
            <xs:element type="con:fieldType" name="field"
                        xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="indecesType">
        <xs:sequence maxOccurs="unbounded" minOccurs="0">
            <xs:element type="con:indexType" name="index">
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="indexType">
        <xs:sequence maxOccurs="unbounded">
            <xs:element type="con:fieldType" name="field"
                        xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
        </xs:sequence>
        <xs:attribute name="type" use="optional" type="con:indexTypeType"/>
    </xs:complexType>

    <xs:complexType name="passwordType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="name" use="required"
                              type="con:fieldNameType" xmlns:con="https://cm5.intertrust.ru/config"/>
                <xs:attribute name="length" use="required" type="xs:string"/>
                <xs:attribute name="not-null" use="optional" type="xs:string"/>
                <xs:attribute name="immutable" use="optional" type="xs:boolean"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="decimalType">
        <xs:sequence>
            <xs:element name="constraints" type="con:constraintsType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
        <xs:attribute name="name" use="required"
                      type="con:fieldNameType" xmlns:con="https://cm5.intertrust.ru/config"/>
        <xs:attribute name="precision" use="optional" type="xs:string"/>
        <xs:attribute name="scale" use="optional" type="xs:string"/>
        <xs:attribute name="not-null" use="optional" type="xs:string"/>
        <xs:attribute name="immutable" use="optional" type="xs:boolean"/>
    </xs:complexType>

    <xs:complexType name="stringType">
        <xs:sequence>
            <xs:element name="constraints" type="con:constraintsType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
        </xs:sequence>
        <xs:attribute name="name" use="required"
                      type="con:fieldNameType" xmlns:con="https://cm5.intertrust.ru/config"/>
