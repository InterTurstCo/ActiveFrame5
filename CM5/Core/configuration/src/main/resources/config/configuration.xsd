            <xs:element name="write" type="con:permissionsType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
            <xs:element name="delete" type="con:permissionsType"
                        maxOccurs="1" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config"/>
