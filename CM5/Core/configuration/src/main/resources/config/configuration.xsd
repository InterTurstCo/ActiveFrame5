    <xs:complexType name="logoScreenType">
        <xs:sequence minOccurs="1">
            <xs:element name="product-title" type="con:productTitleType"/>
        </xs:sequence>
        <xs:attribute name="display-product-version" use="optional" type="xs:boolean"/>
        <xs:attribute name="display-core-version" use="optional" type="xs:boolean"/>
        <xs:attribute name="display-version-list" use="optional" type="xs:boolean"/>
    </xs:complexType>

    <xs:complexType name="sideBarOpeningTimeType">
        <xs:attribute name="default-value" use="optional" type="xs:string"/>
