                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>

            <xs:element type="con:searchAreaRefType" name="search-area-ref"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
            <xs:element type="con:searchCollectionRefType" name="search-collection-ref"
                        maxOccurs="unbounded" minOccurs="0"
                        xmlns:con="https://cm5.intertrust.ru/config">
            </xs:element>
            <xs:element type="con:initialFiltersType" name="initial-filters"
                        xmlns:con="https://cm5.intertrust.ru/config" minOccurs="0"/>
            <xs:element type="con:filterPanelType" name="filter-panel"
                        xmlns:con="https://cm5.intertrust.ru/config" minOccurs="0"/>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="displayChosenValuesType">
