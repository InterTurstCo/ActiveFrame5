<?xml version="1.0" encoding="UTF-8"?>
<module>
    <name>test-sed</name>
    <description>Тестовый модуль системы электронного документооборота</description>
    <depends>
        <depend>core</depend>
    </depends>
    <extension-points-packages>
        <extension-points-package>ru/intertrust/cm/test</extension-points-package>
    </extension-points-packages>
    <gui-components-packages>
        <gui-components-package>ru</gui-components-package>
        <gui-components-package>com</gui-components-package>
    </gui-components-packages>
    <configuration-schema-path>modules-configuration/test-module/custom-configuration.xsd</configuration-schema-path>
    <configuration-paths>
        <configuration-path>modules-configuration/test-module/custom-config.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/domain-objects.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/countries.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/letters.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/unions.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/countries-navigation-panel.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/access.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/workflow.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/global.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/search.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/extendedsearch.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/extendedsearch-int-docs.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/extendedsearch-countries.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/domain-objects-test.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/trigger-events.xml</configuration-path>
        <configuration-path>modules-configuration/test-module/notifications.xml</configuration-path>        
    </configuration-paths>
    <import-files rewrite="true">
        <import-file>importdata/import-organization.csv</import-file>
        <import-file>importdata/import-department.csv</import-file>
        <import-file>importdata/import-employee.csv</import-file>
        <import-file>importdata/set-organization-boss.csv</import-file>
        <import-file>importdata/set-department-boss.csv</import-file>
        <import-file rewrite="false">importdata/import-union.csv</import-file>
        <import-file rewrite="false">importdata/import-country.csv</import-file>
        <import-file>importdata/import-group-members.csv</import-file>
        <import-file rewrite="false">importdata/import-locale.csv</import-file>
        <import-file rewrite="false">importdata/import-notification_text.csv</import-file>
        <import-file>importdata/import-notification.csv</import-file>
    </import-files>

    <import-reports>
        <report-template-dir>reports/all-employee</report-template-dir>

        <!--report-template>
            <template-file>reports/all-employee/template.xml</template-file>
            <template-file>reports/all-employee/all-employee.jrxml</template-file>
        </report-template-->

        <report-template>
            <template-file>reports/all-employee-ds/template.xml</template-file>
            <template-file>reports/all-employee-ds/all-employee-ds.jrxml</template-file>
            <template-file>reports/all-employee-ds/AllEmployeeDs.java</template-file>
        </report-template>

        <report-template>
            <template-file>reports/all-employee-params/template.xml</template-file>
            <template-file>reports/all-employee-params/all-employee-params.jrxml</template-file>
        </report-template>

        <report-template>
            <template-file>reports/all-employee-scriptlet/template.xml</template-file>
            <template-file>reports/all-employee-scriptlet/all-employee-scriptlet.jrxml</template-file>
            <template-file>reports/all-employee-scriptlet/TestScriptlet.java</template-file>
        </report-template>

        <report-template>
            <template-file>reports/employee-groups/template.xml</template-file>
            <template-file>reports/employee-groups/employee-groups.jrxml</template-file>
            <template-file>reports/employee-groups/employee_groups_subreport.jasper</template-file>
            <template-file>reports/employee-groups/employee_groups_subreport.jrxml</template-file>
        </report-template>
    </import-reports>

</module>